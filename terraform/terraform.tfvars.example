# Copy this file to terraform.tfvars and fill in your values

# Cloudflare API token - create at https://dash.cloudflare.com/profile/api-tokens
# Needs permissions: Zone:DNS:Edit for your domain
cloudflare_api_token = "your-cloudflare-api-token-here"

# Your Cloudflare zone ID - find in Cloudflare dashboard for your domain
cloudflare_zone_id = "your-cloudflare-zone-id-here"

# Subdomain for conference server (will create conference.yourdomain.com)
domain_name = "conference.yourdomain.com"

# Your VPS IP address
server_ip = "194.87.103.57"

# Email for Let's Encrypt notifications
acme_email = "your-email@example.com"